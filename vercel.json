{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "routes/api.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "routes/api.js"
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}
